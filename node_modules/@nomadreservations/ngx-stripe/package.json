    "demo:install": "cd demo && yarn install",
    "demo:serve": "run-p demo:watch",
    "demo:watch": "cd demo && npm run start",
    "demo:watch:aot:yalc": "nodemon -e ts -w src -w demo -w public_api.ts -x run-s demo:aot:yalc",
